{
  "version": 2,
  "public": true,
  "github": {
    "silent": true
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/public/index.html"
    },
    {
      "source": "/login",
      "destination": "/public/login.html"
    },
    {
      "source": "/chat",
      "destination": "/public/chat.html"
    },
    {
      "source": "/admin",
      "destination": "/public/admin.html"
    },
    {
      "source": "/dashboard",
      "destination": "/public/dashboard.html"
    },
    {
      "source": "/login.html",
      "destination": "/public/login.html"
    },
    {
      "source": "/chat.html",
      "destination": "/public/chat.html"
    },
    {
      "source": "/admin.html",
      "destination": "/public/admin.html"
    },
    {
      "source": "/dashboard.html",
      "destination": "/public/dashboard.html"
    },
    {
      "source": "/index.html",
      "destination": "/public/index.html"
    },
    {
      "source": "/css/:path*",
      "destination": "/public/css/:path*"
    },
    {
      "source": "/js/:path*",
      "destination": "/public/js/:path*"
    },
    {
      "source": "/img/:path*",
      "destination": "/public/img/:path*"
    },
    {
      "source": "/data/:path*",
      "destination": "/public/data/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/index"
    }
  ],
  "functions": {
    "api/index.py": {
      "runtime": "python3.9",
      "memory": 1024
    }
  },
  "cleanUrls": true,
  "trailingSlash": false
}
